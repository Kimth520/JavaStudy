package inheritanceConstructor2;

public class SuperCar extends Car{

	//필드
	boolean booster;
	
	//메소드
	void boosterOn() {
		System.out.println("부스터 가동");
	}
	void boosterOff() {
		System.out.println("부스터 줄지");
	}
	
//	//생성자
//	public SuperCar() {
//		super();
//	}
	
	
	public SuperCar(boolean booster) {
		super("KIA", "red", 2500);
		this.booster = booster;
	}
}
