package constructorOverloading;
//생성자 오버로딩
public class Person {
	//필드
	String name;
	int age;
	String job;
	
	
	//생성자
	public Person() {
		this.name = "이름없음";
		this.age = 0;
		System.out.println(this);
		System.out.println("기본 생성자가 생성되었습니다");
	}
	
	public Person(String name) {
		this.name = name;
		this.age = 20;//나이의 기본값
		System.out.println("이름은 인수로 받고, "+ "나이는 20살 기본값으로 할당한다");
	}
}
