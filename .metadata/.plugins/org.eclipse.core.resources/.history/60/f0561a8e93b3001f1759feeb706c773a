package inheritanceTest2;

public class AnimalMain {
	public static void main(String[] args) {
		//동물 클래스의 객체 생성
		Animal ani = new Animal();
		//Animal 클래스에 있는 필드와 메소드만 사용 가능
		ani.name = "사자";
		System.out.println(ani.name);
		
		//새 클래스의 객체 생성
		Bird bird = new Bird();
		bird.age = 2;
		bird.type = "종";
		System.out.println(bird.name);
	
	}
}
