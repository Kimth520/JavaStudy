package marvelTask;

public class MarverlHeroMain {
	public static void main(String[] args) {
		//아이언맨과 헐크 객체를 만들고 정보 출력
		MarvelHero ironMan = new MarvelHero("Ironman","변신",47);
		MarvelHero hulk = new MarvelHero("Hulk", "돌연변이", 45);
		ironMan.printInfo();
		hulk.printInfo();
		
		ironMan.performAction();
		hulk.performAction();
		
	}
}
