package method;
//메소드 사용이유1 : 중복된 코드의 제거
public class MethodBasic01 {
	public static void main(String[] args) {
		//동일한 작업을 수행하는 코드를 여러번 작성하는 대신 메소드로 정의하면
		//코드의 가독성과 효율성이 향상더ㅣㄴ다
		
		System.out.println("총합 : " +(5+10));
		System.out.println("총합 : " +(100+200));
		System.out.println("총합 : " +(20+30));
		
		printSum(5,10;)
		
	}
	
	//메소드를 사용한 경우
	static void printSum(int num1, int num2) {
		System.out.println("총합 : "+ (num1 + num2));
	}
	
	
}
