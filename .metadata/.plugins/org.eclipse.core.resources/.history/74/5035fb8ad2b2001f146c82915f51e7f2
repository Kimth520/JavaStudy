package marvelTask;

public class MarverlHeroMain {
	public static void main(String[] args) {
		//아이언맨과 헐크 객체를 만들고 정보 출력
		MarvelHero ironMan = new MarvelHero("Ironman","아이언맨이 강력한 슈트를 착용합니다",47);
		MarvelHero hulk = new MarvelHero("Hulk", "헐크가 미쳐 날뛰고 있습니다", 45);
		ironMan.printInfo();
		hulk.printInfo();
		
		ironMan.performAction();
		hulk.performAction();
		
	}
}
