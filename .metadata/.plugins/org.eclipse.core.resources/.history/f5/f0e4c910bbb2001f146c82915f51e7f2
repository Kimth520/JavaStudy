package constructorBasic2;
//기본 생성자 확인
class Student{
	String name;
	int age;
	
	//기본생성자
	public Student() {
		System.out.println("기본 생성자가 호출되었습니다");
		
	}
}

public class StudentMain {
	public static void main(String[] args) {
		Student st1 = new Student();//객체 생성시 기본 생성자 호출
		System.out.println(st1);
	}
}
